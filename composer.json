{
    "name": "nextbss/vpos",
    "description": "The one stop shop for online payments in Angola, allowing you to process payments requests from EMIS GPO through vPOS.",
    "type": "library",
    "keywords":
        ["vpos",
        "vpos-php",
        "nextbss",
        "payments",
        "angola"
    ],
    "homepage": "https://github.com/nextbss/vpos-php",
    "require": {
        "php": "8.0",
        "phpunit/phpunit": "^9.5",
        "ramsey/uuid": "^4.1",
        "guzzlehttp/guzzle": "^7.2.0"
    },
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Antonio Juca",
            "email": "alexandre.juca@nextbss.co.ao",
            "role": "Software Engineer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vpos\\Vpos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vpos\\VposTest\\": "tests/"
        }
    },
    "scripts": {
        "test": ["vendor/bin/phpunit tests"]
    }
}
